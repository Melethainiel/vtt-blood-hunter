{
  "name": "vtt-blood-hunter",
  "version": "1.0.0",
  "description": "Module Foundry VTT pour automatiser la classe Blood Hunter",
  "main": "scripts/blood-hunter.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:packs": "node scripts/build-packs.mjs",
    "build:all": "npm run build:packs && npm run build",
    "dev": "webpack --mode=development --watch",
    "lint": "eslint scripts/ --ext .js",
    "lint:fix": "eslint scripts/ --ext .js --fix",
    "package": "npm run build:all && zip -r vtt-blood-hunter.zip scripts/ styles/ lang/ packs/ module.json LICENSE README.md -x \"*.git*\" \"node_modules/*\" \"*.log\"",
    "clean": "rm -rf dist/ vtt-blood-hunter.zip packs/*/",
    "validate": "node scripts/validate-module.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Melethainiel/vtt-blood-hunter"
  },
  "keywords": [
    "foundry-vtt",
    "dnd5e",
    "blood-hunter",
    "critical-role"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@foundryvtt/foundryvtt-cli": "^3.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
